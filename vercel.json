{
   "version": 2,
   "name": "option-master",
   "builds": [
      { "src": "index.js", "use": "@vercel/node" }
   ],
   "routes": [
      { "src": "/(.*)", "dest": "/index.js" }
   ],
   "crons": [
     {
       "name": "my-cron-job",
       "command": "node",
       "schedule": "*/5 * * * *",
       "maxExecutions": 1,
       "buildCommand": "npm install && npm run build",
       "output": {
         "format": "json"
       }
     }
   ]
 }
 